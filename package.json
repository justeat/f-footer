{
  "name": "@justeat/f-footer",
  "description": "Fozzie footer â€“ Footer Component for Just Eat projects",
  "version": "0.24.0",
  "main": "dist/js/index.js",
  "files": [
    "dist",
    "src/img",
    "src/scss",
    "src/templates"
  ],
  "homepage": "https://github.com/justeat/f-footer",
  "contributors": [
    "Github contributors <https://github.com/justeat/f-footer/graphs/contributors>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:justeat/f-footer.git"
  },
  "bugs": {
    "url": "https://github.com/justeat/f-footer/issues"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "@justeat/fozzie": "^0.43.0",
    "@justeat/fozzie-colour-palette": "^1.1.0",
    "@kickoff/utils.scss": "2.1.1",
    "include-media": "^1.4.9",
    "lite-ready": "^1.0.4",
    "lodash.debounce": "^4.0.8",
    "qwery": "^4.0.0"
  },
  "devDependencies": {
    "@justeat/gulp-build-fozzie": "^7.19.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "danger": "^3.6.5",
    "gulp": "^3.9.1",
    "js-test-buddy": "^0.0.7"
  },
  "keywords": [
    "fozzie",
    "eyeglass-module"
  ],
  "eyeglass": {
    "name": "f-footer",
    "sassDir": "src/scss",
    "needs": "^1.1.2",
    "exports": false
  },
  "scripts": {
    "prepare": "yarn run lint && yarn run compile",
    "compile": "babel -d dist src",
    "lint": "yarn run lint:css && yarn run lint:js && yarn run test",
    "lint:css": "gulp scss:lint --prod",
    "lint:js": "gulp scripts:lint --prod",
    "test": "gulp scripts:test",
    "test:cover": "gulp scripts:test:coverage"
  },
  "stylelint": {
    "extends": "@justeat/stylelint-config-fozzie"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "Safari >= 8",
    "not ie 10"
  ],
  "assets": [
    {
      "root": "src/",
      "glob": "img/**/*"
    },
    {
      "root": "src/",
      "glob": "templates/**/*",
      "dest": "./"
    }
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "collectCoverageFrom": [
      "src/js/**/*.js"
    ],
    "roots": [
      "src/"
    ]
  }
}
